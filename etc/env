# strategy settings

enabled=true  # вкл/выкл бота
symbol=ASTBTC  # какой тикер торгуем
step=0.1  # шаг в процентах для условия на открытие новой позиции. 0.5 = 0.5%
ticker_amount_digits=1  # До скольки знаков после запятой округлять количество монет в заявке на покупку. Значение можно узнать тут https://www.binance.com/en/trade-rule
ticker_price_digits=0.00000001  # До скольки знаков после запятой округлять цену в заявке. Этот параметр также используется для округления цены при проверке на наличие открытой позиции в этом ценовом диапазоне.
continue_buy_amount=0.0002  # Количество денег, на которое открываем новые позиции. Для SOLUSDT измеряется в USDT, для SOLBTC - в BTC.

global_stop_loss=0.0  # цена, при которой продаём всё и заканчиваем работу

strategy_type=basic  # тип стратегии, floating для плавающей ставки, basic - стратегия с постоянным процентом
avg_rate_sell_limit=1.005  # шаг в процентах для условия сделок. 5% = 1.05

init_buy_amount=1  # сколько позиций открываем в самом начале теста
multiple_sell_on_tick=false # Разрешаем множественные продажи на одном тике или нет.

# trader settings
throttling_time=60  # Минимальная частота тика в секундах
exchange=binance  # Выбор биржи для торгов, bybit или binance
fee_percent=0.075  # Процент комиссий от суммы сделки
dry_run=false  # true - торговля без отправки сделок на бирже
